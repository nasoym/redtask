#!/bin/bash

PARSED_OPTIONS=""
while getopts "p:s:eac:o:" options; do
  PARSED_OPTIONS="${PARSED_OPTIONS} -${options}${OPTARG}"
  case $options in
    o )
        ASSIGN_TO_OWNER=$OPTARG
        ;;
    c )
        CHILD_DEPTH=$OPTARG
        ;;
    p )
        PROJECT_ID=$OPTARG
        ;;
    s )
        SPACES=$OPTARG
        ;;
    e )
        EXTENDED=1
        ;;
    a )
        APPEND=1
        ;;
   \? ) 
        exit 1
        ;;
  esac
done
shift $(( OPTIND - 1 ));


