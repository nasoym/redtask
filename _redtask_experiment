#!/bin/bash

if [ "$#" -ge 1 ]; then

  if [ "$1" = "test_01" ]; then
    RESULT="$(project_member_by_name $2)"
    shift 2
    if [ -n "$RESULT" ];then
      echo "found user: $RESULT"
    else
      echo "no or too many users found"
    fi

  elif [ "$1" = "test_02" ]; then
    #RESULT="$(category_by_name $2)"
    RESULT="$(version_by_name $2)"
    shift 2
    if [ -n "$RESULT" ];then
      echo "found id: $RESULT"
    else
      echo "no or too many ids found"
    fi

  fi

fi
